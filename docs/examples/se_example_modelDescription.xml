<?xml version="1.0" encoding="UTF-8"?>
<fmiModelDescription fmiVersion="3.0-alpha.6" modelName="ThreeInputClocks" instantiationToken="ThreeInputClocks">
	<LogCategories>
		<Category name="logStatusError" description="Log error messages"/>
	</LogCategories>
	<DefaultExperiment startTime="0" stopTime="6" stepSize="0.001"/>
	<ModelVariables>
		<!-- Variables related to input clock 10msClock  -->
		<Float64 name="AIn1" valueReference="0" causality="input" variability="discrete" start="0"/>
		<Float64 name="AIn2" valueReference="1" causality="input" variability="discrete" start="0"/>
		<Float64 name="AOut" valueReference="2" causality="output" variability="discrete"/>

		<!-- Variables related to input clock AperiodicClock  -->
		<Float64 name="BIn"  valueReference="3" causality="input" variability="discrete" start="0"/>
		<Float64 name="BOut" valueReference="4" causality="output" variability="discrete"/>

		<!-- Clock variables -->
		<!-- Periodic input clock -->
		<Clock name="10msClock"      valueReference="5" causality="input"  interval="constant"
		priority="1" intervalCounter="10" resolution="1000"/>
		<!-- Input clock that is triggered by 10msClock -->
		<Clock name="AperiodicClock" valueReference="6" causality="input"  interval="countdown"
		priority="2"/>
		<!-- Output clock -->
		<Clock name="OutputClock"    valueReference="7" causality="output" interval="triggered"/>
		<!-- Periodic input clock -->
		<Clock name="50msClock"      valueReference="8" causality="input"  interval="constant"
		priority="3" intervalCounter="50" resolution="1000"/>
	</ModelVariables>
	<ModelStructure>
		<Output valueReference="2" dependencies="0 1"/>
		<Output valueReference="4" dependencies="3"/>
		<Clock valueReference="5" clockedVariables="2"/>
		<Clock valueReference="6" clockedVariables="4"/>
		<Clock valueReference="7" clockedVariables="5"/>
		<Clock valueReference="8"/>
	</ModelStructure>
</fmiModelDescription>
